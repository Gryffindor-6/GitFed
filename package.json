{
  "name": "git-fed",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/Gryffindor-6/GitFed.git",
  "author": "Gryffindor-6 (https://github.com/Gryffindor-6)",
  "license": "MIT",
  "browserslist": "> 0.25%, not dead",
  "scripts": {
    "start": "concurrently --kill-others-on-fail \"yarn start:server\" \"yarn start:client\"",
    "start:client": "webpack-dev-server --hot --config webpack.config.js",
    "start:server": "nodemon server",
    "test": "jest -i",
    "test:watch": "jest -i --watch",
    "test:update": "jest -i --updateSnapshot",
    "test:coverage": "jest -i --coverage",
    "build": "webpack --config webpack.config.js"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/setupTests.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "cross-fetch": "^2.2.2",
    "express": "^4.16.4",
    "normalize.css": "^8.0.0",
    "pg-promise": "^8.5.1",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-hot-loader": "^4.3.11",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-transform-react-constant-elements": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "concurrently": "^4.0.1",
    "css-loader": "^1.0.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "husky": "^1.1.2",
    "jest": "^23.6.0",
    "lint-staged": "^7.3.0",
    "nodemon": "^1.18.4",
    "prettier": "^1.14.3",
    "redux-devtools-extension": "^2.13.5",
    "regenerator-runtime": "^0.12.1",
    "style-loader": "^0.23.1",
    "superagent": "^4.0.0-beta.5",
    "supertest": "^3.3.0",
    "webpack": "^4.22.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9"
  }
}
